{
  "name": "employees",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build && npm run prisma:prod && node ./server/bin/www",
    "prisma:prod": "cp prisma/schema.production.prisma prisma/schema.prisma && npx prisma generate && npx prisma migrate deploy",
    "build": "cd client && npm install && npm run build",
    "server": "nodemon ./server/bin/www",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "bcrypt": "^5.1.1",
    "concurrently": "^8.2.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "react-cached-handler": "^1.0.7",
    "router": "^2.2.0",
    "sass": "^1.69.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "prisma": "^5.6.0"
  }
}
