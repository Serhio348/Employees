{
  "name": "employees",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build && npx prisma generate && npx prisma migrate deploy && node ./server/bin/www",
    "build": "cd client && npm install --omit=dev && npm run build",
    "server": "nodemon ./server/bin/www",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:reset": "npx prisma migrate reset",
    "db:studio": "npx prisma studio",
    "db:setup": "npm run db:generate && npm run db:migrate"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "bcrypt": "^5.1.1",
    "concurrently": "^8.2.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "react-cached-handler": "^1.0.7",
    "router": "^2.2.0",
    "sass": "^1.69.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "prisma": "^5.6.0"
  }
}
